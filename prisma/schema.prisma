  loc_code         String @default("") @db.Char(5)
  quantity         Float  @default(1)

  @@id([parent, component, workcentre_added, loc_code])
  @@index([component], map: "component")
  @@index([id], map: "id")
  @@index([loc_code], map: "loc_code")
  @@index([parent, loc_code], map: "parent")
  @@index([workcentre_added], map: "workcentre_added")
  @@map("0_bom")
}

model budget_trans {
