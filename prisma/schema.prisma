generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model absentism {
  id               Int       @id @default(autoincrement())
  emp_no           String?   @db.VarChar(200)
  absentism_reason String?   @db.VarChar(200)
  date             DateTime? @db.Date
  trans_id         Int?

  @@unique([emp_no, date, trans_id], map: "pri")
  @@map("0_absentism")
}

model absentism_reasons {
  id           Int     @id @default(autoincrement())
  reason       String? @db.VarChar(200)
  paid         Int?
  abbreviation String? @db.Char(2)
  leave_type   String? @db.VarChar(50)
  days_limit   Float   @default(365)

  @@map("0_absentism_reasons")
}

model alerts {
  id      Int    @id @default(autoincrement())
  bcode   Int
  bname   String @db.VarChar(200)
  uid     Int
  uname   String @db.VarChar(100)
  timein  String @db.VarChar(30)
  timeout String @db.VarChar(30)

  @@map("0_alerts")
}

model areas {
  area_code   Int     @id @default(autoincrement())
  description String  @unique(map: "description") @default("") @db.VarChar(60)
  inactive    Boolean @default(false)
  salesman    String  @db.VarChar(50)

  @@map("0_areas")
}

model areas_locations {
  location_code Int     @id @default(autoincrement())
  description   String  @unique(map: "description") @default("") @db.VarChar(60)
  inactive      Boolean @default(false)
  area          String  @db.VarChar(50)

  @@map("0_areas_locations")
}

model attachments {
  id          Int      @id @default(autoincrement()) @db.UnsignedInt
  description String   @default("") @db.VarChar(60)
  type_no     Int      @default(0)
  trans_no    Int      @default(0)
  unique_name String   @default("") @db.VarChar(60)
  tran_date   DateTime @default(dbgenerated("('0000-00-00')")) @db.Date
  filename    String   @default("") @db.VarChar(60)
  filesize    Int      @default(0)
  filetype    String   @default("") @db.VarChar(60)

  @@index([type_no, trans_no], map: "type_no")
  @@map("0_attachments")
}

model attendace {
  id                Int       @id @default(autoincrement())
  date              DateTime? @db.Date
  name_             String?   @db.VarChar(200)
  work_number       String?   @db.VarChar(50)
  attendance_record String?   @db.Text
  first_record      String?   @db.VarChar(20)
  last_record       String?   @db.VarChar(20)
  total_time        String?   @db.VarChar(20)
  temparature       String    @db.Text

  @@map("0_attendace")
}

model audit_trail {
  id          Int      @id @default(autoincrement())
  type        Int      @default(0) @db.UnsignedSmallInt
  trans_no    Int      @default(0) @db.UnsignedInt
  user        Int      @default(0) @db.UnsignedSmallInt
  stamp       DateTime @default(now()) @db.Timestamp(0)
  description String?  @db.VarChar(60)
  fiscal_year Int      @default(0)
  gl_date     DateTime @default(dbgenerated("('0000-00-00')")) @db.Date
  gl_seq      Int?     @db.UnsignedInt

  @@index([fiscal_year, gl_date, gl_seq], map: "Seq")
  @@index([type, trans_no], map: "Type_and_Number")
  @@index([description], map: "audit_trail_desc_idx")
  @@map("0_audit_trail")
}

model audit_trail_operations {
  id          Int       @id @default(autoincrement())
  trans_type  String?   @db.VarChar(200)
  trans_no    String?   @db.VarChar(100)
  description String?   @db.VarChar(400)
  user        String?   @db.VarChar(200)
  trans_time  DateTime? @db.Timestamp(0)

  @@index([trans_no], map: "op_type")
  @@index([trans_time], map: "time_idx")
  @@index([trans_type], map: "trans_type")
  @@map("0_audit_trail_operations")
}

model bank_accounts {
  account_code             String   @default("") @db.VarChar(15)
  account_type             Int      @default(0) @db.SmallInt
  bank_account_name        String   @default("") @db.VarChar(60)
  bank_account_number      String   @default("") @db.VarChar(100)
  bank_name                String   @default("") @db.VarChar(60)
  bank_address             String?  @db.TinyText
  bank_curr_code           String   @default("") @db.Char(3)
  dflt_curr_act            Boolean  @default(false)
  id                       Int      @id @default(autoincrement()) @db.SmallInt
  bank_charge_act          String   @default("") @db.VarChar(15)
  last_reconciled_date     DateTime @default(dbgenerated("('0000-00-00 00:00:00')")) @db.Timestamp(0)
  ending_reconcile_balance Float    @default(0)
  inactive                 Boolean  @default(false)
  manual_mpesa             Int      @default(0) @db.SmallInt
  ttp                      String   @db.VarChar(50)

  @@index([account_code], map: "account_code")
  @@index([bank_account_name], map: "bank_account_name")
  @@index([bank_account_number], map: "bank_account_number")
  @@map("0_bank_accounts")
}

model bank_trans {
  id             Int       @id @default(autoincrement())
  type           Int?      @db.SmallInt
  trans_no       Int?
  bank_act       String    @default("") @db.VarChar(15)
  ref            String?   @db.VarChar(40)
  trans_date     DateTime  @default(dbgenerated("('0000-00-00')")) @db.Date
  amount         Float?
  dimension_id   Int       @default(0)
  dimension2_id  Int       @default(0)
  person_type_id Int       @default(0)
  person_id      Bytes?    @db.TinyBlob
  reconciled     DateTime? @db.Date
  cheque_no      String?   @db.TinyText
  old_bank_id    Int

  @@index([bank_act, ref], map: "bank_act")
  @@index([bank_act, reconciled], map: "bank_act_2")
  @@index([bank_act, trans_date], map: "bank_act_3")
  @@index([type, trans_no], map: "type")
  @@map("0_bank_trans")
}

model bom {
  id               Int    @default(autoincrement())
  parent           String @default("") @db.Char(20)
  component        String @default("") @db.Char(20)
